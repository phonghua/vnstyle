@model IEnumerable<VnStyle.Services.Business.Models.ImagesByArtist>

<div class="baguetteBoxTwo gallery">
    <div class="gallery">
        @foreach (var img in Model)
        {
            
                <a href="@img.UrlImage" data-caption="galaxies">
                    <img src="@img.UrlImage" />
                </a>
           
        }

    </div>
</div>
<script src="~/Content/js/baguetteBox.js"></script>
<script src="~/Content/js/highlight.min.js"></script>
<script>
        window.onload = function() {
            baguetteBox.run('.gallery', {
                captions: true,       // true|false - Hiển thị tiêu đề ảnh
                buttons: 'auto',      // 'auto'|true|false - Hiển thị nút điều hướng
                async: false,         // true|false - Tải file không đồng bộ
                preload: 2,           // [number] - Tải trước (preload) bao nhiêu ảnh kể từ ảnh hiện tại
                animation: 'slideIn'  // 'slideIn'|'fadeIn'|false - Loại chuyển động

            });
            if (typeof oldIE === 'undefined' && Object.keys) {
                hljs.initHighlighting();
            }
        }
</script>